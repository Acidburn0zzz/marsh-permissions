<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >
    <data class=".ui.app_list.AppListBinding">
        <variable name="appListViewModel" type="com.guavabot.marshpermissions.ui.app_list.AppListViewModel"/>
    </data>
<android.support.v7.widget.RecyclerView
    android:id="@+id/recycler"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="vertical"
    app:items="@{appListViewModel.apps}"
    tools:context=".ui.app_list.AppListActivity"/>
</layout>